\BiSection{声速情况}{Acoustic case}
若在原点处，流体左右状态非常接近，即满足$\Vert \bm{u}_L-\bm{u}_R\Vert\ll1$，从原点处将产生声速波。
在这种情况下，线性系统\eqref{eq:system}的系数将会被简化
\begin{theorem}[Acoustic case]\label{thm:acoustic}
如果 $\bm{u}_L\approx\bm{u}_*\approx\bm{u}_R$,
\begin{equation}\label{eq:coeff-acoustic_up}
\begin{array}{lll}
\displaystyle \frac{\partial u}{\partial t}=-\frac{1}{2}\left[(u_*+c_*)(u^{'}_L+\frac{p^{'}}{\rho_*c_*})+(u_*-c_*)(u^{'}_R-\frac{p^{'}_R}{\rho_*c_*})\right],\\
\displaystyle \frac{\partial p}{\partial t}=-\frac{\rho_* c_*}{2}\left[\left(u_*+c_*\right)\left(u^{'}_L+\frac{p^{'}_L}{\rho_*c_*}\right)-\left(u_*-c_*\right)\left(u^{'}_R-\frac{p^{'}_R}{\rho_*c_*}\right)\right]+\frac{q_x}{\rho_*T_*}\left(\frac{\partial p}{\partial S}\right).
\end{array}
\end{equation}
那么$(\frac{\partial \rho}{\partial t})_*$可以由状态方程$p=p(\rho,S)$计算得到，
\begin{equation}\label{eq:coeff-acoustic_rho}
(\frac{\partial\rho}{\partial t})_*=\left\{
\begin{aligned}
\frac{1}{c^2_*}\left[(\frac{\partial p}{\partial t})_*+u_*(p^{'}_L-c^2_*\rho^{'}_L-\frac{q_x}{\rho_*T_*}\frac{\partial p}{\partial S}(\rho_*,S_*))\right], if~u_*=u_L=u_R>0,\\
\frac{1}{c^2_*}\left[(\frac{\partial p}{\partial t})_*+u_*(p^{'}_R-c^2_*\rho^{'}_R-\frac{q_x}{\rho_*T_*}\frac{\partial p}{\partial S}(\rho_*,S_*))\right], if~u_*=u_L=u_R<0.\\
\end{aligned}
\right.
\end{equation}
\end{theorem}
\begin{proof}
首先，我们考虑向左的声速波。分别用$\bm{u}_-$和$\bm{u}_1$表示$u-c$对应的声速波左、右两边的状态。
因为，穿过声速波，解是连续的，沿着波面对$u$取微分可得
        \begin{equation}
        \frac{\partial u_-}{\partial t}+(u_--c_-)\frac{\partial u_-}{\partial x}= \frac{\partial u_1}{\partial t}+(u_1-c_1)\frac{\partial u_1}{\partial x}.
        \end{equation}
利用方程\eqref{eq:D0pu}可以将上式写作，
        \begin{equation}
        \frac{D u_-}{Dt}-c_-\frac{\partial u_-}{\partial x}=\frac{D u_1}{Dt}+c_1\left(\frac{1}{\rho_1 {c_1}^2}\frac{D p_1}{Dt}-\frac{1}{\rho_1 {c_1}^2}\frac{q_x}{\rho T}\frac{\partial p}{\partial S}\right).
        \end{equation}
 取极限$t\rightarrow 0$ 可得，
        \begin{equation}\label{acoustic_left}
        -\frac{1}{\rho_*}(p_x)_L-c_*(u_x)_L=\left(\frac{{D} u}{{D} t}\right)_* + \frac{u_*-c_*}{\rho_*c_*^2}\left[\left(-\frac{Dp}{Dt}\right)_*+\left(\frac{q_x}{\rho T}\frac{\partial p}{\partial S}\right)_*\right].
        \end{equation}
 对于$u+c$对应的右声速波，采用相同的步骤可得，
        \begin{equation}\label{acoustic_right}
        -\frac{1}{\rho_*}(p_x)_R+c_*(u_x)_R=\left(\frac{{D} u}{{D} t}\right)_*+ \frac{u_*+c_*}{\rho_*c_*^2}\left[\left(-\frac{Dp}{Dt}\right)_*+\left(\frac{q_x}{\rho T}\frac{\partial p}{\partial S}\right)_*\right].
        \end{equation}
由方程\eqref{acoustic_left}和\eqref{acoustic_right}可得
        \begin{equation}\label{acoustic_DPDt}
        \frac{2}{\rho_*c_*^2}\left[\left(\frac{Dp}{Dt}\right)_*-\left(\frac{q_x}{\rho T}\frac{\partial p}{\partial S}\right)_*\right]=-\frac{1}{\rho_*}(p_x)_L-c_*(u_x)_L+\frac{1}{\rho_*}(p_x)_R-c_*(u_x)_R.
        \end{equation}
和
        \begin{equation}\label{acoustic_partial_u}
        \frac{\partial u}{\partial t}=-\frac{1}{2}\left[\left(u_*+c_*\right)(u^{'}_L+\frac{p^{'}_L}{\rho_*c_*})+(u_*-c_*)(u^{'}_R-\frac{p^{'}_R}{\rho_*c_*})\right].
        \end{equation}
注意到有方程
        \begin{equation}\label{acoustic_partial_Du}
        \left(\frac{Du}{Dt}\right)_*=\left(\frac{\partial u}{\partial t}\right)_*+\frac{u_*}{\rho_*c^2_*}\left(-\frac{Dp}{Dt}+\frac{q_x}{\rho T}\frac{\partial p}{\partial S}\right),
        \end{equation}
        \begin{equation}\label{acoustic_partial_Dp}
        \begin{aligned}
        \left(\frac{\partial p}{\partial t}\right)_*=&\left(\frac{D p}{D t}\right)_*-u_*\left(\frac{\partial p}{\partial t}\right)_*\\
        =&\left(\frac{D p}{D t}\right)_*+\rho_*u_*\left(\frac{Du}{Dt}\right)_*\\
        =&-\frac{\rho_* c_*}{2}\left[\left(u_*+c_*\right)\left(u^{'}_L+\frac{p^{'}_L}{\rho_*c_*}\right)-\left(u_*-c_*\right)\left(u^{'}_R-\frac{p^{'}_R}{\rho_*c_*}\right)\right]+\frac{q_x}{\rho_*T_*}\left(\frac{\partial p}{\partial S}\right).
        \end{aligned}
        \end{equation}
那么，我们可得
        \begin{equation}\label{acoustic_partial_p}
        \begin{aligned}
        \left(\frac{\partial p}{\partial t}\right)_*=&\left(\frac{\partial p}{\partial \rho}\frac{\partial\rho}{\partial t}\right)_*+\left(\frac{\partial p }{\partial S}\right)_*\left(\frac{\partial S}{\partial t}_*\right)\\
        =&c^2_*\left(\frac{\partial \rho}{\partial t}\right)_*-u_*\left(p^{'}_L-c^2_*\rho^{'}_L\right)+\left(\frac{q_x}{\rho_* T_*}\frac{\partial p }{\partial S}(\rho_*,S_*)\right).
        \end{aligned}
        \end{equation}
因此有，
        \begin{equation}\label{eq:acoustic_partial_rho}
        (\frac{\partial\rho}{\partial t})_*=\left\{
        \begin{aligned}
        \frac{1}{c^2_*}\left[(\frac{\partial p}{\partial t})_*+u_*(p^{'}_L-c^2_*\rho^{'}_L-\frac{q_x}{\rho_*T_*}\frac{\partial p}{\partial S}(\rho_*,S_*))\right], if~u_*=u_L=u_R>0,\\
        \frac{1}{c^2_*}\left[(\frac{\partial p}{\partial t})_*+u_*(p^{'}_R-c^2_*\rho^{'}_R-\frac{q_x}{\rho_*T_*}\frac{\partial p}{\partial S}(\rho_*,S_*))\right], if~u_*=u_L=u_R<0.\\
        \end{aligned}
        \right.
        \end{equation}
        %\begin{equation}\label{acoustic_partial_rho}
%        \left(\frac{\rho}{\partial t}\right)_*=\left\{
%        \begin{aligned}
%        %\frac{1}{c^2_*}\left[\left(\frac{\partial p}{\partial t}\right)_*\right]
%        \end{aligned}
%        \end{equation}
%The temporal derivative of $\rho$ is obtained by the EOS $p=p(\rho,S)$ while the temporal derivatives of $v$, $\zeta$ and $z$ are obtained by using the third equation in \eqref{eq:D0rhov} and \eqref{eq:D0zetaz}.
\end{proof}
