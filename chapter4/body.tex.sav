下面的引理总结了稀疏波的求解。随着$\alpha\rightarrow 0$，沿着粒子轨迹，压力$p$和速度$u$的导数可以看作是依赖于$\beta$的两个线性方程。
\begin{theorem}\label{lem:rarefaction}
当与特征值$u-c$对应的左波是稀疏波的时候，对于所有的$\beta\in [\beta_L,\beta_{*}]$，物质导数的极限值满足
\begin{equation}\label{eq:rare-abd}
a_L\frac{D u}{D t}(0,\beta) + b_L\frac{D p}{D t}(0,\beta)=d_L,
\end{equation}
这里
\begin{equation}
(a_L,b_L)=\left(1,\frac{1}{\rho(0,\beta)c(0,\beta)}\right)
\end{equation}
且
\begin{equation}\label{eq:d_L}
\begin{aligned}
d_L=&cK(\rho,S)\frac{\partial S}{\partial x}(0,\beta)+\frac{1}{2}\frac{\Theta(\beta_L)}{\Theta(\beta)}(-2c_L\frac{\partial \psi_L}{\partial x}+c_LK(\rho_L,S_L)+G(x,t))\\
-&\frac{\Theta(\beta_L)}{\Theta(\beta)}\int^{\beta}_{\beta_L}\frac{1}{2c(0,\xi)}\frac{\Theta(\beta)}{\Theta(\beta_L)}A(0,\xi)d\xi
+G(x,t),
\end{aligned}
\end{equation}
其中$A(0,\beta)$ 由\eqref{eq:GRP-rare-A-2d}给出.% and $(\psi_x)_L$ is given by \eqref{eq:Gibbs-polytropic} and \eqref{eq:dpsi-polytropic}.
\eqref{eq:rare-abd}中的系数只依赖于初始的梯度$((\bm{u}_x)_L,(\bm{u}_y)_L)$和相应的黎曼解$\bm{u}_*$.
\end{theorem}
对于多方气体，我们有
\begin{equation}
\begin{aligned}
d_L=&\left[\frac{1+\mu^2}{1+2\mu^2}\left(\frac{c(0,\beta)}{c_L}\right)^{\frac{1}{2\mu^2}}+\frac{\mu^2}{1+2\mu^2}\left(\frac{c(0,\beta)}{c_L}\right)^{\frac{1+\mu^2}{\mu^2}}\right]T_LS'_L
-c_L\left(\frac{c(0,\beta)}{c_L}\right)^{\frac{1}{2\mu^2}}\psi'_L\\
+&\frac{\gamma q_x}{2\rho c}\left[\left(\frac{c(0,\beta)}{c_L}\right)^{\frac{1}{2\mu^2}+1}+1\right]
\end{aligned}
\end{equation}
\begin{proof}
由\eqref{eq:D0S}和\eqref{eq:Dpsi-Dtau}，我们可得
\begin{equation}
\frac{Du}{Dt}+\frac{1}{\rho c}\frac{Dp}{Dt}=\frac{D\psi}{Dt}=c K(\rho,S)\frac{\partial S}{\partial x}-c\frac{\partial\psi}{\partial x}.
\end{equation}
通过引理 \ref{lem:dSx}，可得
\begin{equation}\label{eq:GRP-rare-A-2d}
\begin{aligned}
A(0,\beta)&:=c(0,\beta)K(0,\beta)\frac{\partial S}{\partial x}(0,\beta)\\
&=c_LK(0,\beta)\Pi(\beta;\beta_L)^{-2} (S_x)_L.
\end{aligned}
\end{equation}

接下来我们计算 $\displaystyle c(0,\beta)\frac{\partial\psi}{\partial x}(0,\beta)$。
利用与\eqref{eq:S-beta} and \eqref{eq:S-alpha}的推导一样的方法，我们可以将\eqref{eq:Dpsi-Dtau}变化为特征坐标$(\alpha,\beta)$下的方程式
\begin{equation}\label{eq:psi-beta}
\frac{\partial \psi}{\partial \beta} =A(0,\beta)\frac{\partial t}{\partial \beta},
\end{equation}
%
%\begin{equation}
%\begin{aligned}\label{eq:psi-alpha}
%\frac{\partial \psi}{\partial \alpha} = \left(A -c\frac{\partial v}{\partial y}-2c \frac{\partial \psi}{\partial x}+(v_L-v)\frac{\partial\psi}{\partial y}\right)\frac{\partial t}{\partial \alpha}.
%\end{aligned}
%\end{equation}
\iffalse
\begin{equation}\label{eq:psi-beta}
\frac{\partial \psi}{\partial \beta} = \frac{\partial \psi}{\partial t}\frac{\partial t}{\partial \beta} +
\frac{\partial \psi}{\partial x}\frac{\partial x}{\partial \beta} +
\frac{\partial \psi}{\partial y}\frac{dy}{dt}\frac{\partial t}{\partial \beta} = \left(\frac{D_+ \psi}{D t}+(v_L-v)\frac{\partial\psi}{\partial y}\right)\frac{\partial t}{\partial \beta}=
\left(A-c\frac{\partial v}{\partial y}+(v_L-v)\frac{\partial\psi}{\partial y}\right)\frac{\partial t}{\partial \beta},
\end{equation}
and
\begin{equation}
\begin{aligned}\label{eq:psi-alpha}
\frac{\partial \psi}{\partial \alpha} &= \frac{\partial \psi}{\partial t}\frac{\partial t}{\partial \alpha} +
\frac{\partial \psi}{\partial x}\frac{\partial x}{\partial \alpha} +
\frac{\partial \psi}{\partial y}\frac{dy}{dt}\frac{\partial t}{\partial \alpha} = \left(\frac{D_+ \psi}{D t}-2c \frac{\partial \psi}{\partial x}\right)\frac{\partial t}{\partial \alpha}
= \left(A -c\frac{\partial v}{\partial y}-2c \frac{\partial \psi}{\partial x}\right)\frac{\partial t}{\partial \alpha}.
\end{aligned}
\end{equation}
\fi
为了能够得到$\displaystyle c\frac{\partial\psi}{\partial x}$的值，我们需要计算$\displaystyle\frac{\partial\psi}{\partial \alpha}$。
由\eqref{eq:psi-beta}对$\alpha$ 求微分可得
\begin{equation}\label{eq:ODE-psi-0}
\frac{\partial^2 \psi}{\partial \alpha \partial \beta} = A(0,\beta)\frac{\partial^2 t}{\partial \alpha \partial \beta} + \frac{\partial}{\partial \alpha}A(0,\beta)\frac{\partial t}{\partial \beta}.
\end{equation}
令$\alpha$ 趋于$0$，由方程\eqref{eq:tau-beta}和\eqref{eq:Theta-beta}，方程\eqref{eq:ODE-psi-0}可以写作
\begin{equation}\label{eq:ODE-psi}
\frac{\partial}{\partial \beta}\left(\frac{\partial \psi}{\partial \alpha}(0,\beta)\right) =
\frac{A(0,\beta)\Theta(\beta)}{2c(0,\beta,0)}.
\end{equation}
将上式从$\beta_L$ 到$\beta$求积分可得
\begin{equation}
\frac{\partial \psi}{\partial \alpha}(0,\beta) = \frac{\partial \psi}{\partial \alpha}(0,\beta_L) + \frac{1}{2}\int_{\beta_L}^{\beta}\frac{A(0,\xi)}{c(0,\xi)}
\Theta(\xi)d\xi.
\end{equation}\\
(i) 计算初值 $\frac{\partial \psi}{\partial \alpha}(0,\beta_L)$.
注意到
\begin{equation}
\frac{\partial\psi}{\partial\alpha}=\frac{\partial t}{\partial \alpha}[\frac{\partial \psi }{\partial t}+(u-c)\frac{\partial \psi}{\partial x}],
\end{equation}
将(\ref{eq:dpsi})代入并令$\beta=\beta_L$，我们有
\begin{equation}
\frac{\partial\psi}{\partial\alpha}(0,\beta_L)=\Theta(\beta_L)[-2c_L\psi^{'}_L+c_LK(\rho_L,S_L)S^{'}_L+G(x,t)].
\end{equation}
%\begin{equation}
%\frac{\partial \psi}{\partial \alpha}(0,\beta_L)
%= \left(A(0,\beta_L) -c_L (v_y)_L -2c(\psi_x)_L\right)\Theta(\beta_L).
%\end{equation}
%Therefore,
(ii) 返回到$(x,t)$坐标中.
\begin{equation}\label{eq:GRP-rare-Psi_x-2d}
\begin{aligned}
 &2c(0,\beta)\frac{\partial\psi}{\partial x}(0,\beta)\\[3mm]
=&cK(\rho,S)\frac{\partial S}{\partial x}(0,\beta)-\Theta^{-1}(\beta)\frac{\partial\psi}{\partial \alpha}+G(x,t)\\[3mm]
=&cK(\rho,S)\frac{\partial S}{\partial x}(0,\beta)-\frac{\Theta(\beta_L)}{\Theta(\beta)}(-2c_L\frac{\partial \psi_L}{\partial x}+c_LK(\rho_L,S_L)+G(x,t))\\
-&\frac{\Theta(\beta_L)}{\Theta(\beta)}\int^{\beta}_{\beta_L}\frac{1}{2c(0,\xi)}\frac{\Theta(\beta)}{\Theta(\beta_L)}A(0,\xi)d\xi
+G(x,t).
\end{aligned}
\end{equation}
最终，可得
\begin{equation}\label{eq:dL}
\begin{aligned}
d_L(\beta) :=&\frac{D \psi}{D t}(0,\beta,0)
= A(0,\beta,0)- c(0,\beta,0)\frac{\partial \psi}{\partial x}(0,\beta,0)+G(x,t)\\[2.5mm]
=&cK(\rho,S)\frac{\partial S}{\partial x}(0,\beta)+\frac{1}{2}\frac{\Theta(\beta_L)}{\Theta(\beta)}(-2c_L\frac{\partial \psi_L}{\partial x}+c_LK(\rho_L,S_L)+G(x,t))\\
-&\frac{\Theta(\beta_L)}{\Theta(\beta)}\int^{\beta}_{\beta_L}\frac{1}{2c(0,\xi)}\frac{\Theta(\beta)}{\Theta(\beta_L)}A(0,\xi)d\xi
+G(x,t).
\end{aligned}
\end{equation}
这里$A(0,\beta)$ 由\eqref{eq:GRP-rare-A-2d}给出.
\end{proof}
\vspace{2mm}

\BiSection{激波求解}{Resolution of the shock}

在这一节中，我们考虑右激波的求解。
令$x=x(t)$ 是与特征值为$u+c$相对应的激波，波速为$\sigma=x'(t)$。$\overline{M}(t) = \overline{M}(x(t)+,y(t),t)$ 和$M(t) = M(x(t)-,y(t),t)$表示任意变量$M$在激波前和激波后的值。穿过激波面，关于$u$-$p$的$Rankine-Hugonint$ 关系式为
\begin{equation}\label{eq:RH-pu}
u=\overline{u}+\Phi(p;\overline{p},\overline{\rho}).
\end{equation}
激波面的传播速度为
\begin{equation}\label{eq:sigma}
\sigma = \frac{dx}{dt} = \frac{\rho u - \overline{\rho}\overline{u}}{\rho - \overline{\rho}}.
\end{equation}
沿着激波$x(t)$，$Rankine-Hugonint$关系式\eqref{eq:RH-pu}的方向导数为，
\begin{equation}\label{eq:RH-u-p}
\left(\frac{\partial}{\partial t}+\sigma\frac{\partial}{\partial x}\right)(u-\overline{u}-\Phi(p;\overline{p},\overline{\rho}))=0.
\end{equation}
并且沿着激波，取$t\rightarrow 0+$的极限可得
\begin{equation}
(\rho,u,p)\rightarrow (\rho_{2*},u_*,p_*),\quad
(\overline{\rho},\overline{u},\overline{p})\rightarrow (\rho_R,u_R,p_R),
\end{equation}
和
\begin{equation}
\frac{Du}{Dt}\rightarrow \left(\frac{Du}{Dt}\right)_{2*},\quad
\frac{Dp}{Dt}\rightarrow \left(\frac{Dp}{Dt}\right)_{2*}.
\end{equation}
 $(D u/D t)_{2*}$ 和$(D p/D t)_{2*}$的极限值满足接下来的线性关系式。
\begin{theorem}\label{lem:shock}
当与特征值$u+c$对应的右波是激波时，物质导数的极限满足
\begin{equation}\label{eq:a-b-d-R}
a_R\left(\frac{D u}{D t}\right)_{2*} + b_R\left(\frac{D p}{D t}\right)_{2*} = d_R,
\end{equation}
其中
\begin{equation}
(a_R,b_R)=\left(1+\rho_{2*}(\sigma-u_*)\Phi_1,-\frac{\sigma-u_*}{\rho_{2*}c_{2*}^2}-\Phi_1\right),
\end{equation}
\begin{equation}
d_R = L_p^R (p_x)_R + L_u^R (u_x)_R + L_\rho^R (\rho_x)_R + L_q^R,
\end{equation}
和
\begin{align*}
&L_p^R = -\frac{1}{\rho_R} + (\sigma-u_R)\Phi_2,&&
L_u^R = \sigma-u_R-\rho_R c_R^2 \Phi_2 - \rho_R\Phi_3,\\
&L_\rho^R = (\sigma-u_R)\Phi_3, &&
L_q^R = (\gamma-1)[\Phi_2q_x-\frac{(\sigma-u)q_x }{\rho c^2}].
\end{align*}
这里， $\sigma$ 由公式\eqref{eq:sigma}给出，并且$\Phi_l$, $l=1,2,3$满足
\begin{equation}
\Phi_1 = \frac{\partial \Phi}{\partial p}(p_*;p_R,\rho_R),\quad
\Phi_2 = \frac{\partial \Phi}{\partial \overline{p}}(p_*;p_R,\rho_R),\quad
\Phi_3 = \frac{\partial \Phi}{\partial \overline{\rho}}(p_*;p_R,\rho_R).
\end{equation}
\end{theorem}

\begin{proof}
\begin{equation}\label{eq:u}
\begin{aligned}
&\frac{\partial u}{\partial t}+\sigma\frac{\partial u}{\partial x}\\
=&\frac{Du}{Dt}+(\sigma-u)\frac{\partial u}{\partial x}\\
=&\frac{Du}{Dt}+\frac{1}{\rho c^2}(\sigma-u)\frac{q_x}{\rho T}\frac{\partial p}{\partial S}-\frac{1}{\rho c^2}(\sigma-u)\frac{Dp}{Dt}.
\end{aligned}
\end{equation}
\begin{equation}\label{eq:p}
\begin{aligned}
&\frac{\partial p}{\partial t}+\sigma\frac{\partial p}{\partial x}\\
=&\frac{Dp}{Dt}+(\sigma-u)\frac{\partial p}{\partial x}\\
=&\frac{Dp}{Dt}-\rho(\sigma-u)\frac{Du}{Dt}.
\end{aligned}
\end{equation}
由方程\eqref{eq:RH-u-p}，通过链式法则，我们可得
\begin{equation}\label{eq:a-b-d-R-primitive}
\begin{aligned}
&\left(\frac{\partial u}{\partial t}+\sigma\frac{\partial u}{\partial x}\right)
- \frac{\partial \Phi}{\partial p}\left(\frac{\partial p}{\partial t}+\sigma\frac{\partial p}{\partial x}\right) \\
=&
\left(\frac{\partial\overline{u}}{\partial t}+\sigma\frac{\partial \overline{u}}{\partial x}\right)
+ \frac{\partial \Phi}{\partial \overline{p}}\left(\frac{\partial \overline{p}}{\partial t}+\sigma\frac{\partial \overline{p}}{\partial x}\right)
+ \frac{\partial \Phi}{\partial \overline{\rho}}\left(\frac{\partial \overline{\rho}}{\partial t}+\sigma\frac{\partial \overline{\rho}}{\partial x}\right).
\end{aligned}
\end{equation}
将\eqref{eq:u}和\eqref{eq:p}替换到\eqref{eq:a-b-d-R-primitive}的左端可得
\begin{align*}
&\left(\frac{\partial u}{\partial t}+\sigma\frac{\partial u}{\partial x}\right)
- \frac{\partial \Phi}{\partial p}\left(\frac{\partial p}{\partial t}+\sigma\frac{\partial p}{\partial x}\right) \\
=&\frac{D u}{D t}+(\sigma-u)\frac{\partial u}{\partial x}
- \frac{\partial \Phi}{\partial p}\left[\frac{D p}{D t}+(\sigma-u)\frac{\partial p}{\partial x}\right]\\
=&\frac{D u}{D t}+(\sigma-u)\left(-\frac{1}{\rho  c^2}\frac{D p}{D t} + \frac{1}{\rho  c^2}\frac{q_x}{\rho T}\frac{\partial p}{\partial S}\right)
- \frac{\partial \Phi}{\partial p}\left[\frac{D p}{D t}-(\sigma-u)\rho\frac{D u}{D t}\right].
%=&[1+\rho(\sigma-u)\Phi_1]\frac{D u}{D t}
%-\left(\frac{\sigma-u}{\rho c^2}+\Phi_1\right)\frac{D p}{D t} - (\sigma-u)(v_y)_R.
\end{align*}
将\eqref{eq:D0pu}和\eqref{eq:D0rhov}替换到 \eqref{eq:a-b-d-R-primitive}的右端可得
\begin{align*}
&\left(\frac{\partial\overline{u}}{\partial t}+\sigma\frac{\partial \overline{u}}{\partial x}\right)
+ \frac{\partial \Phi}{\partial \overline{p}}\left(\frac{\partial \overline{p}}{\partial t}+\sigma\frac{\partial \overline{p}}{\partial x}\right)
+ \frac{\partial \Phi}{\partial \overline{\rho}}\left(\frac{\partial \overline{\rho}}{\partial t}+\sigma\frac{\partial \overline{\rho}}{\partial x}\right)\\
=&\frac{D \overline{u}}{D t}+(\sigma-\overline{u})\frac{\partial \overline{u}}{\partial x}
+ \frac{\partial \Phi}{\partial \overline{p}}\left(\frac{D \overline{p}}{D t}+(\sigma-\overline{u})\frac{\partial \overline{p}}{\partial x}\right)
+ \frac{\partial \Phi}{\partial \overline{\rho}}\left(\frac{D \overline{\rho}}{D t}+(\sigma-\overline{u})\frac{\partial \overline{\rho}}{\partial x}\right)\\
=&-\frac{1}{\overline{\rho}} \frac{\partial \overline{p}}{\partial x} +(\sigma-\overline{u})\frac{\partial \overline{u}}{\partial x}
+ \frac{\partial \Phi}{\partial \overline{p}}\left[-\overline{\rho}\overline{c}^2\left(\frac{\partial \overline{u}}{\partial x}+\frac{q_x}{\overline{\rho} \overline{T}}\frac{\partial \overline{p}}{\partial \overline{S}}\right)+(\sigma-\overline{u})\frac{\partial \overline{p}}{\partial x}\right]\\
&+ \frac{\partial \Phi}{\partial \overline{\rho}}\left[-\overline{\rho}\left(\frac{\partial \overline{u}}{\partial x}\right)+(\sigma-\overline{u})\frac{\partial \overline{\rho}}{\partial x}\right].
\end{align*}
由上面两个方程并取$t\rightarrow 0+$的极限可以证明该引理。
\end{proof}
\vspace{2mm}

\begin{remark}
对于多方气体，我们有
\begin{align*}
&\Phi(p;\overline{p},\overline{\rho}) = (p-\overline{p})\sqrt{\frac{1-\mu^2}{\overline{\rho}(p+\mu^2\overline{p})}},\\
&\Phi_1=\frac{1}{2}\sqrt{\frac{1-\mu^2}{\rho_R(p_*+\mu^2 p_R)}}\frac{p_*+(1+2\mu^2)p_R}{p_*+\mu^2 p_R},\\
&\Phi_2=-\frac{1}{2}\sqrt{\frac{1-\mu^2}{\rho_R(p_*+\mu^2 p_R)}}\frac{(2+\mu^2)p_*+\mu^2 p_R}{p_*+\mu^2 p_R},\\
&\Phi_3=-\frac{p_*-p_R}{2 p_R}\sqrt{\frac{1-\mu^2}{\rho_R(p_*+\mu^2 p_R)}},\\
&L_q^R=q_x(\gamma-1)\left[\Phi_2-(\sigma-u_*)\frac{1}{\rho_{2*}c_{2*}^2}\right].
\end{align*}
其中$\mu^2=\frac{\gamma-1}{\gamma+1}$.
\end{remark}

\vspace{2mm}

\BiSection{奇点处解的时间导数}{Temporal derivatives of the solution at the singularity}
前面两个小节中的结果用于计算原点$(x,t)=(0,0$时解的时间导数。

\begin{theorem}[Nonsonic case]\label{lem:Dup-Dt}
假定$t$-轴位于左波和右波中间，$\left({D} u/{D} t\right)_*$ 和 $\left({D} p/{D} t\right)_*$在中间区域的极限值可以通过求解下面的线性系统得到，
\begin{equation}\label{eq:system}
\begin{aligned}
a_L\left(\frac{{D} u}{{D} t}\right)_* + b_L\left(\frac{{D} p}{{D} t}\right)_* &= {d}_L-K(\rho,S)\frac{DS}{Dt},~~~~~~~(rarefaction)\\
a_R\left(\frac{{D} u}{{D} t}\right)_* + b_R\left(\frac{{D} p}{{D} t}\right)_* &= {d}_R,~~~~~~~(shock)
\end{aligned}
\end{equation}
系数$a_L,b_L,d_L,a_R,b_R$ 和 $d_R$ 可以由引理 \ref{lem:rarefaction}得到。
\end{theorem}
\vspace{2mm}

\begin{theorem}[Nonsonic case]\label{thm:temporal-pu-nonsonic}
假定$t$-轴位于左波和右波中间，$p$ 和$u$ 的时间导数在中间区域的极限值有以下关系
\begin{align*}
\left(\frac{\partial u}{\partial t}\right)_* &= \left(\frac{{D} u}{{D} t}\right)_* + \frac{u_*}{\rho_{*} c_{*}^2}\left(\frac{D p}{D t}\right)_* + \frac{u_*q_x}{\rho_*^2c_*^2T} \left(\frac{\partial p}{\partial S}\right)_*,\\
\left(\frac{\partial p}{\partial t}\right)_* &= \left(\frac{{D} p}{{D} t}\right)_* + \rho_{*} u_*\left(\frac{D u}{D t}\right)_*.
\end{align*}
\end{theorem}
\begin{proof}
由\eqref{eq:D0pu} 和 \eqref{eq:D0rhov}可得，
\begin{align*}
\frac{\partial u}{\partial t} &= \frac{{D} u}{{D} t} - u\frac{\partial u}{\partial x}
=\frac{{D} u}{{D} t} - u\left(-\frac{1}{\rho c^2}\frac{D p}{D t}+\frac{q_x}{\rho^2c^2T} \left(\frac{\partial p}{\partial S}\right)\right)
= \frac{{D} u}{{D} t} + \frac{u}{\rho c^2}\frac{D p}{D t} - \frac{uq_x}{\rho^2c^2T} \left(\frac{\partial p}{\partial S}\right),\\
\frac{\partial p}{\partial t} &= \frac{{D} p}{{D} t} - u\frac{\partial p}{\partial x}
=\frac{{D} p}{{D} t} - u \left(-\rho \frac{D u}{D t}\right)
= \frac{{D} p}{{D} t} + \rho u \frac{D u}{D t}.
\end{align*}
通过估计上面两个方程在原点的值来得到最终的结果。
\end{proof}
\vspace{2mm}

当$t$-轴位于稀疏波内部时，属于声速情况。
在这种情况下，$(\partial u/\partial t)_*$ 和$(\partial p/\partial t)_*$的计算将会变得简单。
\begin{theorem}[Sonic case]\label{thm:temporal-pu-sonic}
假定$t$-轴位于$u-c$对应的左稀疏波内部。那么$p$ 和$u$的时间导数满足以下关系
\begin{align}
\left(\frac{\partial u}{\partial t}\right)_* &= d_L (0)-\frac{q_x}{\rho^2cT}\left(\frac{\partial p}{\partial S}\right)_\rho \\
\left(\frac{\partial p}{\partial t}\right)_* &= \rho_* u_* d_L (0).
\end{align}
\end{theorem}
\vspace{2mm}

下一步，我们计算密度的时间导数值。
\begin{equation}
\left(\frac{\partial\rho}{\partial t}\right)_*=\lim\limits_{t\rightarrow 0+}\frac{\partial\rho}{\partial t}(0,t).
\end{equation}
%can be calculated from the equation of state $p=p(\rho,S)$.
因为穿过接触间断$\rho$是间断的，所以$(\partial\rho/\partial t)_*$依赖于接触间断的方向。
$\rho$-$p$的$Rankine-Hugonint$关系满足
\begin{equation}
\rho = H(p;\overline{p},\overline{\rho}).
\end{equation}
沿着激波，求微分可得
\begin{equation}\label{eq:RH-rho-p}
\left(\frac{\partial}{\partial t}+\sigma\frac{\partial}{\partial x}\right)(\rho - H(p;\overline{p},\overline{\rho}))=0.
\end{equation}
可通过接下来的定理计算该结果。

\begin{theorem}\label{thm:drho-dt}
密度时间导数$\left(\partial \rho/\partial t\right)_*$的计算依赖于$u_*$的符号。
\begin{itemize}
\item[(1)] 如果 $u_* > 0$，可通过下面公式计算
\begin{equation}\label{eq:drho-dt}
\left(\frac{\partial \rho}{\partial t}\right)_* = \frac{1}{c_*^2}\left[\left(\frac{\partial p}{\partial t}\right)_* +\frac{q_x}{\rho T}\frac{\partial p}{\partial S} +\frac{\partial p}{\partial S}(\rho_*,S_*)\frac{u_*}{c_*K(\rho_*S_*)A(0,\beta_*)}\right].
\end{equation}
\item[(2)] 如果 $u_* < 0$, 可通过以下方程求解
\begin{equation}\label{eq:drho-dt-shock}
g_\rho^R\left(\frac{\partial \rho}{\partial t}\right)_*
+ g_p^R\left(\frac{D p}{D t}\right)_*
+ g_u^R \left(\frac{D u}{D t}\right)_*
= f_R,
\end{equation}
其中
\begin{equation}
\begin{array}{l}
\displaystyle g_\rho^R = u_*-\sigma,\quad
g_p^R = \frac{\sigma}{c_{2*}^2}-u_* H_1,\quad
g_u^R = u_* \rho_{2*} (\sigma-u_*)H_1,\\[2mm]
\displaystyle f_R = u_*(\sigma-u_R) H_2 (p_x)_R + u_*(\sigma-u_R)H_3(\rho_x)_R - u_*\rho_R(c_R^2 H_2 + H_3)(u_x)_R+u_*H_2(\gamma-1)q_x\\[2mm]
\displaystyle \ \ \ \ \ \ \ +\frac{\sigma}{\rho u T c^2}\frac{\partial p}{\partial S}.\\
\end{array}
\end{equation}
这里， $\sigma$是激波的初始速度，且$H_i$， $i=1,2,3$， 可以定义为
\begin{equation}
H_1 = \frac{\partial H}{\partial p}(p_*;p_R,\rho_R),\quad
H_2 = \frac{\partial H}{\partial \overline{p}}(p_*;p_R,\rho_R),\quad
H_3 = \frac{\partial H}{\partial \overline{\rho}}(p_*;p_R,\rho_R).
\end{equation}
\end{itemize}
\end{theorem}

\begin{proof}
对于$u_* > 0$情况, 由 \eqref{eq:D0S}和 \eqref{eq:p-EOS}，我们可以计算得
\begin{align*}
\left(\frac{\partial \rho}{\partial t}+u\frac{\partial \rho}{\partial x}\right)_*= \frac{1}{{c_{1*}}^2}\left[\frac{\partial p}{\partial t}+u\frac{\partial p}{\partial x} - \frac{\partial p}{\partial S}\left(\frac{\partial S}{\partial t}+u\frac{\partial S}{\partial x}\right)\right]_*
= \frac{1}{{c_{1*}}^2}\left(\frac{\partial p}{\partial t}+u\frac{\partial p}{\partial x} + u\frac{\partial p}{\partial S}\frac{\partial S}{\partial x}\right)_*.
\end{align*}
并且$(\partial S/\partial x)_*$ 可由\eqref{eq:dSx}给出，\eqref{eq:drho-dt}的结论即可证明。\\

当$u_* < 0$情况下，与定理\ref{lem:shock}的证明相同。对\eqref{eq:RH-rho-p}使用链式法则，并由控制方程 \eqref{eq:D0pu}和\eqref{eq:D0rhov}可证。
\iffalse
with the same manner used to prove Lemma \ref{lem:shock}, use the chain rule to expand \eqref{eq:RH-rho-p} which leads to
\begin{equation}
\begin{aligned}\label{eq:RH-rho-p-directional-deri}
&\left(\frac{\partial \rho}{\partial t}+\sigma\frac{\partial \rho}{\partial x} + v\frac{\partial \rho}{\partial y}\right) - \frac{\partial H}{\partial p}\left(\frac{\partial p}{\partial t}+\sigma\frac{\partial p}{\partial x}+v\frac{\partial p}{\partial y}\right)\\
=& \frac{\partial H}{\partial \overline{p}}\left(\frac{\partial \overline{p}}{\partial t}+\sigma\frac{\partial \overline{p}}{\partial x}+v\frac{\partial \overline{p}}{\partial y}\right)
+ \frac{\partial H}{\partial \overline{\rho}}\left(\frac{\partial \overline{\rho}}{\partial t}+\sigma\frac{\partial \overline{\rho}}{\partial x}+v\frac{\partial \overline{\rho}}{\partial y}\right).
\end{aligned}
\end{equation}
Substituting \eqref{eq:D0pu} into  the left hand side of \eqref{eq:RH-rho-p-directional-deri},
\begin{align*}
&u\left[\left(\frac{\partial \rho}{\partial t}+\sigma\frac{\partial \rho}{\partial x} + v\frac{\partial \rho}{\partial y}\right)
- \frac{\partial H}{\partial p}\left(\frac{\partial p}{\partial t}+\sigma\frac{\partial p}{\partial x}+v\frac{\partial p}{\partial y}\right)\right] \\
=&(u-\sigma)\left(\frac{\partial \rho}{\partial t}+ v\frac{\partial \rho}{\partial y}\right)+\sigma\frac{D \rho}{D t} -u \frac{\partial H}{\partial p} \left[\frac{D p}{D t}+(\sigma-u)\frac{\partial p}{\partial x}\right]\\
=&(u-\sigma)\left(\frac{\partial \rho}{\partial t}+ v\frac{\partial \rho}{\partial y}\right)+\frac{\sigma}{c^2}\left(\frac{D p}{D t}-\frac{\partial p}{\partial S}\frac{D S}{D t}\right)
- u\frac{\partial H}{\partial p}\left[\frac{D p}{D t}-(\sigma-u)\rho\frac{D u}{D t}\right]\\
=&(u-\sigma)\left(\frac{\partial \rho}{\partial t}+ v\frac{\partial \rho}{\partial y}\right)+\left(\frac{\sigma}{c^2}
- u \frac{\partial H}{\partial p}\right)\frac{D p}{D t}-u \rho(\sigma-u)\frac{\partial H}{\partial p}\frac{D u}{D t}.
\end{align*}
Substituting \eqref{eq:D0pu} and \eqref{eq:D0rhov} into  the right hand side of \eqref{eq:RH-rho-p-directional-deri},
\begin{align*}
&\frac{\partial H}{\partial \overline{p}}\left(\frac{\partial \overline{p}}{\partial t}+\sigma\frac{\partial \overline{p}}{\partial x}+v\frac{\partial \overline{p}}{\partial y}\right)
+ \frac{\partial H}{\partial \overline{\rho}}\left(\frac{\partial \overline{\rho}}{\partial t}+\sigma\frac{\partial \overline{\rho}}{\partial x}+v\frac{\partial \overline{\rho}}{\partial y}\right)\\
=&\frac{\partial H}{\partial \overline{p}}\left[\frac{D \overline{p}}{D t}+(\sigma-\overline{u})\frac{\partial \overline{p}}{\partial x}\right]
+ \frac{\partial H}{\partial \overline{\rho}}\left[\frac{D \overline{\rho}}{D t}+(\sigma-\overline{u})\frac{\partial \overline{\rho}}{\partial x}\right]\\
=&\frac{\partial H}{\partial \overline{p}}\left[-\overline{\rho}\overline{c}^2\left(\frac{\partial \overline{u}}{\partial x}+\frac{\partial \overline{v}}{\partial y}\right)+(\sigma-\overline{u})\frac{\partial \overline{p}}{\partial x}\right]
+ \frac{\partial H}{\partial \overline{\rho}}\left[-\overline{\rho} \left(\frac{\partial \overline{u}}{\partial x}+\frac{\partial \overline{v}}{\partial y}\right)+(\sigma-\overline{u})\frac{\partial \overline{\rho}}{\partial x}\right].
\end{align*}
The final result \eqref{eq:drho-dt-shock} is obtained by combining the above two equations and taking the limit $t\rightarrow 0+$.
\fi
\end{proof}
